<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Colors-->

    <Color x:Key="UniversityColor">#FF00669E</Color>
    <Color x:Key="AccentColor">#FF00BDB5</Color>
    <Color x:Key="SystemAccentColor">#FF00BDB5</Color>

    <!--#endregion-->

    <!--#region Brushes-->

    <SolidColorBrush x:Key="UniversityColorBrush" Color="{StaticResource UniversityColor}" />
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="SystemAccentColorBrush" Color="{ThemeResource SystemAccentColor}" />

    <SolidColorBrush x:Key="EnrolledBrush" Color="{StaticResource AccentColor}" />

    <SolidColorBrush x:Key="QueuedBrush" Color="{StaticResource AccentColor}" Opacity="0.5" />

    <!--#endregion-->

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrastBlack">
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrastWhite">
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
